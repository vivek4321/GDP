<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


  

<hibernate-mapping>
    <class name="com.GDP.test.model.Paper" table="papers" schema="gdpbackenddatabase">
        
        <!-- <id name="paperID" column="paperid"></id> -->
        
        <id name="paperID">
			<column name="paperid"/>
			<generator class="increment"/>
		</id>
        
        <property name="paperTitle" column="papertitle"></property>
        <property name="paperDataFile" column="paperdatafile"></property>
        <property name="abstractOfThePaper" column="abstractofthepaper"></property>                     
       
    </class>
    
</hibernate-mapping>